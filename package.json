{
  "name": "async-validator",
  "version": "4.2.5",
  "description": "validate form asynchronous",
  "typings": "typings/index.d.ts",
  "type": "module",
  "keywords": [
    "validator",
    "validate",
    "async"
  ],
  "homepage": "https://github.com/btea/async-validator",
  "repository": {
    "type": "git",
    "url": "git@github.com:btea/async-validator.git"
  },
  "bugs": {
    "url": "https://github.com/btea/async-validator/issues"
  },
  "license": "MIT",
  "scripts": {
    "lint-staged": "lint-staged",
    "postinstall": "simple-git-hooks",
    "format": "prettier --write --cache .",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "build": "tsdown"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "lint-staged": "^16.0.0",
    "oxlint": "^0.16.11",
    "prettier": "^3.5.3",
    "simple-git-hooks": "^2.13.0",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache"
    ],
    "*.{tsx,js,jsx,ts}": [
      "oxlint --fix"
    ]
  },
  "packageManager": "pnpm@10.10.0"
}
